# Frontend Environment Configuration - EXAMPLE FILE
# Copy this file to .env and fill in your actual values
# WARNING: Do NOT commit .env with real secrets to git!

# Backend API URL - set this to your deployed backend URL
REACT_APP_BACKEND_URL=https://glass-defect-detection-prototype-production.up.railway.app

# Supabase Configuration (ANON key is safe to be public)
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# Admin Configuration (loaded from environment variables only)
REACT_APP_ADMIN_EMAILS=admin@example.com
