{
  "name": "native-glass",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:emu": "expo start --localhost",
    "start:lan": "expo start --lan",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --localhost --android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "build:dev": "node -e \"console.error('DEVELOPMENT BUILD DISABLED. Use npm run start:emu for Expo Go.'); process.exit(1)\"",
    "build:android": "eas build --profile production --platform android",
    "build:ios": "eas build --profile production --platform ios",
    "update:preview": "eas update --branch preview --message 'preview update'",
    "update:production": "eas update --branch production --message 'production update'",
    "submit:android": "eas submit -p android --latest",
    "submit:ios": "eas submit -p ios --latest"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@supabase/supabase-js": "^2.45.2",
    "buffer": "^6.0.3",
    "expo": "54.0.23",
    "expo-camera": "~17.0.9",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.18",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.14",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "jpeg-js": "^0.4.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-pytorch-core": "^0.2.4",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eas-cli": "^16.27.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
